<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.linjia.mapper.changeinfoMapper">

    <insert id="changeinfo" parameterType="com.linjia.pojo.Uuser">
        UPDATE uuser SET  address=#{address},`name`=#{name},let=#{let} WHERE id=#{id}
    </insert>




</mapper>
